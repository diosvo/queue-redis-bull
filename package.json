{
  "name": "queue-redis-bull",
  "version": "1.0.0",
  "description": "demo queue",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "nyc --reporter=lcov --reporter=text mocha tests/*.js --exit"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "cache": true,
    "all": true,
    "include": [
      "controllers/*.js",
      "queues/*.js",
      "processes/*.js"
    ],
    "exclude": [
      "*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diosvo/queue-redis-bull.git"
  },
  "author": "diosvo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diosvo/queue-redis-bull/issues"
  },
  "homepage": "https://github.com/diosvo/queue-redis-bull#readme",
  "dependencies": {
    "bull": "^4.10.4",
    "express": "^4.18.2",
    "redis": "^4.6.5"
  },
  "devDependencies": {
    "@bull-board/api": "^5.0.1",
    "@bull-board/express": "^5.0.1",
    "@bull-board/ui": "^5.0.1",
    "axios": "^1.3.6",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "node-mailer": "^0.1.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "sinon": "^15.0.4",
    "swagger-ui-express": "^4.6.2"
  }
}

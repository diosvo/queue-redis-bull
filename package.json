{
  "name": "queue-redis-bull",
  "version": "1.0.0",
  "description": "demo queue",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "nyc --reporter=lcov --reporter=text mocha tests/*.js --exit"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "cache": true,
    "all": true,
    "include": [
      "controllers/*.js",
      "queues/*.js",
      "processes/*.js"
    ],
    "exclude": [
      "*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diosvo/queue-redis-bull.git"
  },
  "author": "diosvo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diosvo/queue-redis-bull/issues"
  },
  "homepage": "https://github.com/diosvo/queue-redis-bull#readme",
  "dependencies": {
    "bull": "^4.8.3",
    "express": "^4.18.1",
    "redis": "^4.1.0"
  },
  "devDependencies": {
    "@bull-board/api": "^3.11.0",
    "@bull-board/express": "^3.11.0",
    "@bull-board/ui": "^3.11.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "mocha": "^10.0.0",
    "node-mailer": "^0.1.1",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0"
  }
}